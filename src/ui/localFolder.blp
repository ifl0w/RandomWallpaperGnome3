using Gtk 4.0;
using Adw 1;

template $LocalFolderSettings: Adw.PreferencesPage {
    Adw.PreferencesGroup {
        title: _("General");

        Adw.EntryRow folder_row {
            title: _("Folder");

            Button folder {
                valign: center;

                Adw.ButtonContent {
                    icon-name: "folder-open-symbolic";
                }
            }
        }
    }
}
